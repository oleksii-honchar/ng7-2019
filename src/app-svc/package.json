    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.3.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.0.0",
    "babel-loader": "8",
    "babel-plugin-module-resolver": "3",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.6.0",
